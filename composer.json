{
    "name": "alagunto/ez-schedules",
    "description": "This one allows you to manage any kind of schedules ez-ly in Laravel 5.5+",
    "type": "lirbrary",
    "license": "MIT",
    "authors": [
        {
            "name": "Alagunto",
            "email": "priah@mail.ru"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/console": "~5.5.0|~5.6.0",
        "illuminate/contracts": "~5.5.0|~5.6.0",
        "illuminate/events": "~5.5.0|~5.6.0",
        "illuminate/filesystem": "~5.5.0|~5.6.0",
        "illuminate/support": "~5.5.0|~5.6.0",
        "illuminate/database": "~5.5.0|~5.6.0",
        "illuminate/notifications": "~5.5.0|~5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5 || ^7.0",
        "orchestra/testbench" : "~3.5.0|~3.6.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Alagunto\\EzSchedules\\": "src"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Alagunto\\EzSchedules\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alagunto\\EzSchedules\\SchedulesProvider"
            ]
        }
    }
}
